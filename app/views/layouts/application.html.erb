<!DOCTYPE html>
<html>
  <head>
    <title>SurfLeague</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div id="container" class="effect mainnav-lg">
      <%= render 'templates/navbar' %>
      <div class="boxed">
        <div id="content-container">
          <div id="page-content">
            <div class="row">
              <div class="col-lg-12">
                  <%= yield %>
              </div>
            </div>
          </div>
        </div>
        <%= render 'templates/sidebar' %>
      </div>
    </div>
  </body>
</html>
