<!DOCTYPE html>
<html>
  <head>
    <title>SurfLeague</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div>
      <%= render 'templates/navbar' %>  
    </div>
    <div class="row">
      <div class="col-xs-2">
        <%= render 'templates/sidebar' %>
      </div>
      <div class="col-xs-10">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
